<Application
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" 
	x:Class="Uberball.Game.Client.App"
	xmlns:Local="clr-namespace:Uberball.Game.Client">
	
	<Application.Resources>
		<ResourceDictionary>
			<ResourceDictionary.MergedDictionaries>
				<ResourceDictionary Source="/Uberball.Game.Client.Areas.MatchArea;component/Views/Common.xaml" />
				<ResourceDictionary Source="/Uberball.Game.Client.Areas.MatchArea;component/Views/Player.xaml" />
			</ResourceDictionary.MergedDictionaries>
			<Local:ViewModelLocator x:Key="ViewModelLocator"/>
			<Local:DesignerViewModelLocator x:Key="DesignerViewModelLocator"/>
		</ResourceDictionary>
	</Application.Resources>
</Application>
