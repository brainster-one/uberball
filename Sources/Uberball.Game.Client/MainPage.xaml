<UserControl x:Class="Uberball.Game.Client.MainPage"
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
	xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
	mc:Ignorable="d"
	d:DesignHeight="300" d:DesignWidth="400" DataContext="{Binding Source={StaticResource ViewModelLocator}, Path=MatchViewModel}">
	<Grid>
		<ListBox Margin="114,0,125,208" ItemsSource="{Binding Path=Entities}" />
		<Button Command="{Binding Path=Connect}" Content="{Binding Path=ConnectionState}" Height="39" Width="96" Margin="12,0,292,260" />
	</Grid>
</UserControl>
